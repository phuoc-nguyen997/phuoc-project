  <script>

	  $('#btnAddToCart').click(function(){
		var maSP = $(this).attr('maSP');
		var quantity = parseInt($('#quantity').val());
		  var tenSP = $('#tenSP').attr('tenSP');
		  var imageSP = $('#imageSP').attr('imageSP');
		  var dongia = parseInt($('#dongia').attr('dongia'));
		if(cart[maSP]){
			var oldItem = cart[maSP];
			oldItem.quantity += quantity;
			oldItem.tongtien = (oldItem.quantity * dongia);
		}else{
			cart[maSP] = {
				'maSP':maSP,
				'tenSP': tenSP,
				'giaSP': dongia,
				'imageSP': imageSP,
				'quantity' : quantity,
				'tongtien': (quantity * dongia)
			}
		}
	  	setCookie('cart',JSON.stringify(cart),1);
		alert('Them vao gio hang thanh cong');
	  });

		$(document).ready(function(){

		var quantitiy=0;
		   $('.quantity-right-plus').click(function(e){
		        
		        // Stop acting like a button
		        e.preventDefault();
		        // Get the field name
		        var quantity = parseInt($('#quantity').val());
		        
		        // If is not undefined
		            
		            $('#quantity').val(quantity + 1);

		          
		            // Increment
		        
		    });

		     $('.quantity-left-minus').click(function(e){
		        // Stop acting like a button
		        e.preventDefault();
		        // Get the field name
		        var quantity = parseInt($('#quantity').val());
		        
		        // If is not undefined
		      
		            // Increment
		            if(quantity>0){
		            $('#quantity').val(quantity - 1);
		            }
		    });
		    
		});
	</script>
    
